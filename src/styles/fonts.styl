font-large-extra()
  font-family ALS-Sector, Roboto, monospace
  line-height 1.2
  font-size clamp(1.5rem, calc(2vw + 1.5rem), 2rem)
font-large()
  font-family ALS-Sector, Roboto, monospace
  line-height 1.2
  font-size clamp(1rem, calc(1vw + 1rem), 1.5rem)
font-medium()
  font-family ALS-Sector, Roboto, monospace
  line-height 1.2
  font-size clamp(1rem, calc(0.7vw + 0.7rem), 1.3rem)
font-small()
  font-family ALS-Sector, Roboto, monospace
  line-height 1.2
  font-size clamp(0.8rem, calc(0.5vw + 0.7rem), 1rem)
font-small-extra()
  font-family ALS-Sector, Roboto, monospace
  line-height 1.2
  font-size clamp(0.6rem, calc(0.3vw + 0.7rem), 0.8rem)


font-bold()
  font-weight 700
font-normal()
  font-weight 500
font-thin()
  font-weight 400
